{
  "name": "iteaoffice/affiliation",
  "type": "library",
  "license": "proprietary",
  "description": "Code to manage affiliation",
  "keywords": [
    "affiliation"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "zendframework/zendframework": "^3.1"
  },
  "require-dev": {
    "iteaoffice/testing": "dev-master",
    "iteaoffice/contact": "^3.1",
    "iteaoffice/organisation": "^3.1",
    "iteaoffice/general": "^3.1",
    "iteaoffice/project": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Affiliation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AffiliationTest\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev",
      "dev-develop": "3.1.x-dev"
    }
  }
}
