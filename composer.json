{
  "name": "iteaoffice/affiliation",
  "type": "library",
  "license": "proprietary",
  "description": "General code for  Affiliations",
  "keywords": [
    "affiliation"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "zendframework/zendframework": "~3.0"
  },
  "config": {
    "secure-http": false
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev",
      "dev-develop": "3.1-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Affiliation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AffiliationTest\\": "tests"
    }
  }
}
