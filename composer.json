{
  "name": "iteaoffice/affiliation",
  "type": "library",
  "license": "proprietary",
  "description": "General code for  Affiliations",
  "keywords": [
    "affiliation"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7",
    "zendframework/zendframework": "~3.0"
  },
  "require-dev": {
    "iteaoffice/testing": "dev-master",
    "iteaoffice/organisation": "dev-develop",
    "iteaoffice/project": "dev-develop",
    "doctrine/doctrine-orm-module": "~1.1",
    "pdepend/pdepend": "@stable",
    "phpmd/phpmd": "@stable",
    "sebastian/phpcpd": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "friendsofphp/php-cs-fixer": "@stable",
    "jakub-onderka/php-parallel-lint": "@stable",
    "jakub-onderka/php-console-highlighter": "@stable"
  },
  "config": {
    "secure-http": false
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev",
      "dev-develop": "3.1-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Affiliation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AffiliationTest\\": "test"
    }
  }
}
