{% do form.prepare() %}
{{ form().openTag(form)|raw }}
{{ formelement(form.get('csrf')) }}

{% set questionnaireFieldset = form.get('affiliation_entity_questionnaire_questionnaire') %}

{{ lbs5formelement(questionnaireFieldset.get('questionnaire')) }}
{{ lbs5formelement(questionnaireFieldset.get('description')) }}
{{ lbs5formelement(questionnaireFieldset.get('organisationType')) }}
{{ lbs5formelement(questionnaireFieldset.get('programCall')) }}
{{ lbs5formelement(questionnaireFieldset.get('phase')) }}

{% if not edit %}
    <div class="row mb-3">
        <div class="offset-sm-3 col-sm-9">
            {{ formelement(form.get('submit')) }}
            {{ formelement(form.get('cancel')) }}
        </div>
    </div>
    {{ form().closeTag()|raw }}
{% endif %}
