{% do headTitle().append(translate("txt-admin")) %}
{% do headTitle().append(translate("txt-affiliation")) %}
{% do headTitle().append(translate("txt-affiliation-question-category-%s")|format(category)) %}

<h1>{{ translate("txt-affiliation-question-category-%s")|format(category) }} {{ affiliationQuestionCategoryLink(category,'edit','button') }}</h1>

<dl class="dl-horizontal">
    <dt>{{ translate("txt-category-id") }}</dt>
    <dd>{{ category.id }}</dd>
    <dt>{{ translate("txt-sequence") }}</dt>
    <dd>{{ category.sequence }}</dd>
    <dt>{{ translate("txt-category") }}</dt>
    <dd>{{ category.category }}</dd>
    <dt>{{ translate("txt-questions") }}</dt>
    <dd>{{ category.questions|length }}</dd>
</dl>